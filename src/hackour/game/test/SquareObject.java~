package hackour.game.test;

import hackour.game.*;

public class SquareObject implements GObject{
	private int side;
	
	private int x;
	private int y;
	
	public SquareObject( int side, int x, int y ){
		this.side = side;
		this.x = x;
		this.y = y;
	}
	public SquareObject( int x, int y ){
		this( 15, x, y );
	}
	public SquareObject(){
		this(0,0);
	}
	
	public void doTick( Game g ){}
	
	public int getX(){
		return x;
	}
	public int getY(){
		return y;
	}
	public int getSide(){
		return side;
	}
}